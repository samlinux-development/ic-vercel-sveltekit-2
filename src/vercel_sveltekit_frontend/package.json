{
  "name": "vercel_sveltekit_frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create vercel_sveltekit_backend && dfx generate vercel_sveltekit_backend && dfx deploy",
    "start": "vite --port 3000",
    "prebuild": "dfx generate",
    "build": "tsc && vite build",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\""
  },
  "dependencies": {
    "@dfinity/agent": "^1.0.1",
    "@sveltejs/adapter-vercel": "^5.1.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "dotenv": "^16.3.1",
    "sass": "^1.63.6",
    "svelte": "^4.2.9",
    "svelte-check": "^3.6.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-environment": "^1.1.3"
  }
}
